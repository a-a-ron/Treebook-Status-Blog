
<div class="row"><br /><br />
  <div class="col-md-4">

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "well"}) do |f| %>
    <%= devise_error_messages! %>
    <fieldset>
      <legend><h2>Sign up</h2></legend>

      <div><%= f.label :first_name %><br />
        <%= f.text_field :first_name %></div><br />

      <div><%= f.label :last_name %><br />
        <%= f.text_field :last_name %></div><br />

      <div><%= f.label :profile_name %><br />
        <%= f.text_field :profile_name %></div><br />

      <div><%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true %></div><br />

      <div><%= f.label :password %><br />
        <%= f.password_field :password, autocomplete: "off" %></div><br />

      <div><%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off" %></div><br />

      <div><%= f.submit "Sign up", :class => "btn btn-info" %></div>
    </fieldset>
  <% end %>
  <br />

  <%= render "devise/shared/links" %>
</div>
